{
  "name": "handwritten-ocr-5-o3",
  "version": "1.0.0",
  "description": "Handwritten OCR System v5 with o3 model",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "lint": "eslint . --quiet",
    "lint:warn": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx src/cli.ts",
    "analyze": "tsx src/cli.ts analyze"
  },
  "bin": {
    "pdf-ocr-analyzer": "./dist/cli.js"
  },
  "keywords": [
    "ocr",
    "handwritten",
    "text-extraction"
  ],
  "author": "Toshiaki Ohmae",
  "license": "MIT",
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.29.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "dotenv": "^16.6.0",
    "openai": "^5.8.1",
    "pdf-img-convert": "^2.0.0"
  }
}
