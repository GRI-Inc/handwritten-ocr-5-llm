{
  "name": "@handwritten-ocr/vision-module",
  "version": "1.0.0",
  "description": "Reusable vision analysis module using OpenAI Vision API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vision",
    "ocr",
    "openai",
    "image-analysis",
    "typescript"
  ],
  "author": "Toshiaki Ohmae",
  "license": "MIT",
  "dependencies": {
    "openai": "^5.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "dotenv": "^17.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}